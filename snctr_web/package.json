{
  "name": "SIN-CENTRO",
  "version": "1.0.0",
  "description": "Esta es una webapp para dibujo en vivo colaborativo y decentralizado",
  "private": false,
  "scripts": {
    "build": "NODE_ENV=production nwb build-web-app && rm -rf dist/*.map",
    "clean": "nwb clean-app",
    "start": "nwb serve-web-app --reload",
