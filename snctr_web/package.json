    "fast-fuzzy": "^1.8.3",
    "hammerjs": "^2.0.8",
    "inferno": "^7.1.1",
    "jquery": "^3.3.1",
    "nouislider": "^12.1.0",
    "npm": "^6.6.0",    
    "popper.js": "^1.14.7"    
  },
  "devDependencies": {
    "nwb": "^0.25.2"
  },
